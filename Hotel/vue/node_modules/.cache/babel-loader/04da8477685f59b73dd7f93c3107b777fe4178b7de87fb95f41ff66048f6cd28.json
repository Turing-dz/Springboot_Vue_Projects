    this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.loadOrders(pageNum)\r\n    },\r\n    del(id) {   // 单个删除\r\n      this.$confirm('您确定删除吗？', '确认删除', {type: \"warning\"}).then(response => {\r\n        this.$request.delete('/orders/delete/' + id).then(res => {\r\n          if (res.code === '200') {   // 表示操作成功\r\n            this.$message.success('操作成功')\r\n            this.loadOrders(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n\r\n  }\r\n}\r\n</script>"],"mappings":"AAgCA;EACAA,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,WAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,WAAA;EACA;EACAC,OAAA;IACAD,YAAAJ,OAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAM,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAR,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAQ,MAAA,OAAAf,IAAA,CAAAgB;QACA;MACA,GAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,UAAA,GAAAF,GAAA,CAAAnB,IAAA,EAAAsB,IAAA;UACA,KAAAb,KAAA,GAAAU,GAAA,CAAAnB,IAAA,EAAAS,KAAA;QACA;UACA,KAAAc,QAAA,CAAAC,KAAA,CAAAL,GAAA,CAAAM,GAAA;QACA;MACA;IACA;IACAC,oBAAAnB,OAAA;MACA,KAAAoB,UAAA,CAAApB,OAAA;IACA;IACAqB,IAAAX,EAAA;MAAA;MACA,KAAAY,QAAA;QAAAC,IAAA;MAAA,GAAAZ,IAAA,CAAAa,QAAA;QACA,KAAAlB,QAAA,CAAAmB,MAAA,qBAAAf,EAAA,EAAAC,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YAAA;YACA,KAAAG,QAAA,CAAAU,OAAA;YACA,KAAAN,UAAA;UACA;YACA,KAAAJ,QAAA,CAAAC,KAAA,CAAAL,GAAA,CAAAM,GAAA;UACA;QACA;MACA,GAAAS,KAAA,QACA;IACA;EAEA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}